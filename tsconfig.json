{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components"
      ],
      "@/lib": [
        "src/lib"
      ],
      "@/icons": [
        "src/icons"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}